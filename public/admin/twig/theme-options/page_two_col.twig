{% extends "base_page.twig" %}
{% block layout %}
    <div id="post-body" class="metabox-holder columns-2">
        <div>
            {{ description }}
        </div>
        <hr class="wp-clearfix">
        <div id="postbox-container-1" class=" post-box-container">
            {{ do_meta_boxes( metabox_right.screen, metabox_right.action, metabox_right.object ) }}
        </div>
        <div id="postbox-container-2" class="postbox-container">
            {{ do_meta_boxes( metabox_left.screen, metabox_left.action, metabox_left.object ) }}
        </div>
    </div>
{% endblock %}
